<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="othelloApp.OthelloController" fx:id="root" spacing="10">
    <VBox spacing="15">
        <Button fx:id="startScene" mnemonicParsing="false" onAction="#startGame" text="Change Scene" />
        <Button text="Start Game" onAction="#startGame"/>
        <Button text="End Game" onAction="#endGame"/>
        <Button text="Settings" onAction="#settings"/>
    </VBox>
</HBox>



